{
  "name": "@dblechoc/e2e",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "start": "yarn --cwd ../../ start"
  },
  "devDependencies": {
    "jest": "25.1.0",
    "jest-puppeteer": "4.4.0",
    "puppeteer": "2.1.1"
  },
  "jest": {
    "preset": "jest-puppeteer",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  }
}
